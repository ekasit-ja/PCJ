<%if(models.length < 1) {%>
    <p class="alert alert-warning text-center"><%=__("no-model-available")%></p>
<%} else {%>
    <h3 class="header"><%=__("model-you-can-choose")%></h3>
    <div class="row row-flex">
        <% _.each(models, function(model, i) { %>
            <div class="col-md-4 col-sm-6 col-xs-12">
                <a href="<%= url.replace(':mid', model.id) %>" class="btn-model">
                    <div class="img-frame"><img src="<%= model.image %>"></div>
                    <div class="title"><%= model.title %></div>
                </a>
            </div>
        <% }) %>
    </div>
<%}%>
