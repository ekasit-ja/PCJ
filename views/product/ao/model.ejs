<div class="container">
    <%if(model) {%>
        <h3 class="header"><%=__("type")%> : <%= model.type.title %></h3>
        <h4 class="header"><%=__("model")%> : <%= model.title %></h4>

        <%- partial("../../base/fotorama.ejs") %>

        <h3 class="header"><%=__("option-you-can-choose")%></h3>
        <div class="option-container">
            <div class="row">
                <div class="col-sm-2 col-sm-offset-2 col-xs-2 option-label"><%=__("size")%></div>
                <div class="col-sm-6 col-xs-10 option-detail"><%=__("available-upon-request")%></div>
            </div>
        </div>
        <br>

        <%- partial("../../base/product_tab.ejs") %>
        <%- partial("../../base/quote_button.ejs") %>

    <%} else {%>
        <br>
        <p class="alert alert-warning text-center">There is no requested model.</p>
    <%}%>
</div>
